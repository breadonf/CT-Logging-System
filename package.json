    "cross-conf-env": "^1.2.1",
    "cross-env": "^7.0.3",
    "draft-convert": "^2.1.12",
    "draft-js": "^0.11.7",
    "draftjs-to-html": "^0.9.1",
    "formik": "^2.2.9",
    "html-react-parser": "^3.0.1",
    "html-to-draftjs": "^1.5.0",
    "next": "12.0.7",
